{
    "name": "vue-pronto",
    "version": "1.9.0-1",
    "description": "Seriously fast vue server renderer",
    "main": "lib/index.js",
    "publishConfig": {
        "tag": "webpack"
    },
    "files": [
        "lib/**/*.js",
        ".babelrc"
    ],
    "dependencies": {
        "autoprefixer": "^9.1.5",
        "babel-loader": "^8.0.5",
        "browserify": "^16.2.3",
        "clean-css": "^4.2.1",
        "css-loader": "^2.1.0",
        "deepmerge": "^3.1.0",
        "find": "^0.2.9",
        "find-node-modules": "^1.0.4",
        "js-to-string": "^0.4.5",
        "lru-cache": "^5.1.1",
        "object-hash": "^1.3.0",
        "require-from-string": "^2.0.2",
        "uglify-js": "^3.4.8",
        "vue": "^2.5.22",
        "vue-loader": "^15.5.1",
        "vue-server-renderer": "^2.5.22",
        "vue-template-compiler": "^2.5.22",
        "vueify": "^9.4.1",
        "webpack": "^4.29.0",
        "webpack-chain": "^5.2.0",
        "webpack-merge": "^4.2.1",
        "xss": "^1.0.3"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-es2015": "^7.0.0-beta.53",
        "@types/autoprefixer": "^9.1.1",
        "@types/clean-css": "^3.4.30",
        "@types/lru-cache": "^4.1.1",
        "@types/memory-fs": "^0.3.2",
        "@types/mkdirp": "^0.5.2",
        "@types/node": "^10.9.2",
        "@types/object-hash": "^1.2.0",
        "@types/require-from-string": "^1.2.0",
        "@types/uglify-js": "^3.0.3",
        "@types/webpack": "^4.4.23",
        "@types/webpack-merge": "^4.1.3",
        "ava": "^1.2.0",
        "axios": "^0.18.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "codecov": "^3.0.4",
        "express": "^4.16.3",
        "flame": "^0.1.20",
        "generate-release": "^1.1.1",
        "nodemon": "^1.18.3",
        "nyc": "^11.9.0",
        "simple-vue-component-test": "^1.0.0",
        "sleep-ms": "^2.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.2.4"
    },
    "scripts": {
        "release": "npm test && generate-release",
        "debug": "nodemon --inspect tests/example/index-webpack.js",
        "start": "node tests/example/index-webpack.js",
        "debug-windows": "npm run build-windows && node --inspect tests/example/index-webpack.js",
        "start-windows": "npm run build-windows && node tests/example/index-webpack.js",
        "debug-vueify": "nodemon --inspect tests/example/index-vueify.js",
        "start-vueify": "node tests/example/index-vueify.js",
        "debug-vueify-windows": "npm run build-windows && node --inspect tests/example/index-vueify.js",
        "start-vueify-windows": "npm run build-windows && node tests/example/index-vueify.js",
        "lint": "tslint --project tsconfig.json",
        "pretest": "npm run lint",
        "test": "TEST=true nyc ava",
        "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
        "preversion": "npm test"
    },
    "ava": {
        "files": [
            "tests/**/*.js",
            "!tests/example/**/*.js"
        ],
        "sources": [
            "lib/**/*.js"
        ],
        "concurrency": 5,
        "failFast": false,
        "powerAssert": false,
        "babel": {
            "testOptions": {
                "babelrc": false,
                "configFile": false
            }
        }
    },
    "nyc": {
        "include": [
            "lib/**/*.js"
        ],
        "exclude": [
            "lib/assets/**/*"
        ],
        "reporter": [
            "text-summary"
        ]
    },
    "author": "Daniel Cherubini",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/express-vue/vue-pronto.git"
    },
    "keywords": [
        "vue",
        "express",
        "node",
        "ssr",
        "javascript",
        "stream",
        "io"
    ],
    "bugs": {
        "url": "https://github.com/express-vue/vue-pronto/issues"
    },
    "homepage": "https://github.com/express-vue/vue-pronto#readme"
}
